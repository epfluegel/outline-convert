        <outline text="PROJECTS" _note="#slide ">
            <outline text="IDS Game ">
                <outline text="This has moved to: https://dynalist.io/d/-7vZEOuyaA5s189akLMBjwNH#z=NaoRKTgquBGW5hEup0e4qG6- "/>
