                <outline text="Utiliy matrix: ">
                    <outline text="$$\begin{array}{ c | c | c | c }  \mathcal{U} \downarrow \mathcal{A}\rightarrow &amp; s^\mathcal{A}_{a_1} &amp; s^\mathcal{A}_{a_2}  &amp; s^{\mathcal{A}}_{- a} \\  s^{\mathcal{U}}_{d_1} &amp;  - c^{\mathcal{U}}_{d_{1,1}} -L_1l^\mathcal{U}_{1}, L_1l^\mathcal{U}_{1}- c^{\mathcal{A}}_{1} &amp; -c^{\mathcal{U}}_{d_{1,2}} -L_1 l^\mathcal{U}_{2},  L_1l^\mathcal{U}_{2} - c^\mathcal{A}_{2} &amp; -c^\mathcal{U}_{d_{1,2}} ,0\\  s^{\mathcal{U}}_{d_2} &amp;   - c^{\mathcal{U}}_{d_{2,1}}-L_2 l^\mathcal{U}_{1},  L_2l^{\mathcal{U}}_{1}- c^{\mathcal{A}}_{1} &amp;  -c^\mathcal{U}_{d_{2,2}}-L_2 l^\mathcal{U}_{2},  L_2 l^\mathcal{U}_{2} - c^\mathcal{A}_{2} &amp; -c^\mathcal{U}_{d_{2,2}},0 \end{array}$$"/>
                </outline>
                <outline text="Assumptions: ">
                    <outline text="$$c^\mathcal{A}_{1} &lt; c^\mathcal{A}_{2}$$ without loss of generality "/>
                    <outline text="$$c^\mathcal{U}_{d_{1,1}} &lt; l^\mathcal{U}_{1} $$ "/>
                    <outline text="$$c^\mathcal{U}_{d_{2,1}}  &lt; l^\mathcal{U}_{1}$$ "/>
                    <outline text="$$c^\mathcal{U}_{d_{1,2}} &lt; l^\mathcal{U}_{2} $$ "/>
                    <outline text="$$c^\mathcal{U}_{d_{2,2}}  &lt; l^\mathcal{U}_{2}$$ "/>
                    <outline text="$$ c^\mathcal{U}_{d_{1,1}} &lt; c^\mathcal{U}_{d_{2,1}} $$"/>
                    <outline text="$$ c^\mathcal{U}_{d_{1,2}} &lt; c^\mathcal{U}_{d_{2,2}} $$"/>
                </outline>
